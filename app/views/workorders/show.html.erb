<p id="notice"><%= notice %></p>

<table class="table table-striped">
	<thead>
		<tr>
			<th>KEY</th>
			<th>VALUE</th>
		</tr>
	</thead>

	<tbody>
		<tr>
			<td>ID</td>
			<td><%= @workorder[:id] %></td>
		</tr>

		<tr>
			<td>DATA SET</td>
			<td><a href="/assets/<%= @workorder[:data_set] %>" target="_blank"><%= @workorder[:data_set] %></a></td>
