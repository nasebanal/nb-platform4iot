		<tr>
			<td>STATUS</td>
			<td><%= @workorder[:status] %></td>
		</tr>

		<tr>
			<td>CREATOR</td>
			<td><%= @workorder[:creator] %></td>
		</tr>

		<tr>
			<td>CREATED AT</td>
			<td><%= @workorder[:created_at] %></td>
		</tr>

		<tr>
			<td>UPDATED AT</td>
			<td><%= @workorder[:updated_at] %></td>
